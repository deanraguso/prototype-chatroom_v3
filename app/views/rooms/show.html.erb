<p id="notice"><%= notice %></p>

<%= link_to 'Edit', edit_room_path(@room) %> |
<%= link_to 'Back', rooms_path %>

<br>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="messages">
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.text %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h1>New Message</h1>

<%= render 'messages/form', message: @message %>